{
  "name": "vector-score",
  "version": "1.0.3",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "music",
    "notation",
    "sheet music",
    "svg",
    "typescript",
    "staff",
    "rhythm"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/vector-score.umd.cjs",
  "module": "./dist/vector-score.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vector-score.js",
      "require": "./dist/vector-score.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "jsdom": "^27.3.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  }
}
